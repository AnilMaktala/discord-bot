{
  "name": "@amplify-discord-bots/app",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview"
  },
  "dependencies": {
    "aws-amplify": "^4.3.3"
