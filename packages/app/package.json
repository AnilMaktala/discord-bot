  "devDependencies": {
    "@amplify-discord-bots/aws-exports": "*",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.3.7",
    "carbon-components-svelte": "^0.47.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
